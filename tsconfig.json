{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "lib": ["es2017", "dom"],
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "paths": {
      "src/*": ["src/*"]
    }
  }
}
